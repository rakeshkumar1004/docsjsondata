    "sections": [
      {
        "heading": {
          "chunk_references": [
            "fd8ed806-b481-4d1c-aba5-de118e81cabd"
          ],
          "value": "What is a project?"
        },
        "content": {
          "chunk_references": [
            "1d0d095b-8522-49ef-8d03-bddbb3ddf57f"
          ],
          "value": "A project is a workspace that groups documents, users, and processing history. Projects help teams organize work and keep content separated by product, team, or initiative."
        }
      },
      {
        "heading": {
          "chunk_references": [
            "995424d9-ffb7-4a67-9ee1-4301b48bc5de"
          ],
          "value": "Can I work on multiple projects at the same time?"
        },
        "content": {
          "chunk_references": [
            "540439d9-caab-41c3-af51-4752b4150852"
          ],
          "value": "Yes. Users can switch between projects freely, and each project maintains its own content and history."
        }
      },
      {
        "heading": {
          "chunk_references": [
            "ffad110d-96df-4f4f-93cd-3b3b3bf1651f"
          ],
          "value": "What happens if I lose access to a project?"
        },
        "content": {
          "chunk_references": [
            "cd021fd4-2597-4835-96b0-3ef7e51fee6d"
          ],
          "value": "If access is removed, the project and its documents are no longer visible. This change takes effect immediately to protect content."
        }
      }
    ]
  }
