      {
        "heading": {
          "chunk_references": [
            "1b4d0d20-abad-4122-942c-75b2164096ea"
          ],
          "value": "What is DocPublisher?"
        },
        "content": {
          "chunk_references": [
            "594142e7-8560-4749-8423-5379edb4cdf6"
          ],
          "value": "DocPublisher turns structured content into publishable outputs such as web help or downloadable formats."
        }
      },
      {
        "heading": {
          "chunk_references": [
            "9c99ed24-abe4-4987-a249-436a0b8cef9a"
          ],
          "value": "When should I publish content?"
        },
        "content": {
          "chunk_references": [
            "97da935b-7af7-4383-9bac-ae8e23bd2d5f"
          ],
          "value": "Publishing works best after content has been reviewed and approved. Any later changes require republishing."
        }
      },
      {
        "heading": {
          "chunk_references": [
            "32837531-f341-4212-9e65-95c6d1b60aca"
          ],
          "value": "How do I know publishing was successful?"
        },
        "content": {
          "chunk_references": [
            "33472c19-4f8d-4bee-ac1c-8af62d4bcda9"
          ],
          "value": "Publishing status and results are clearly shown. If issues occur, messages explain what needs attention."
        }
      }
    ]
