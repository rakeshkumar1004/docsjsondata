    "sections": [
      {
        "heading": {
          "chunk_references": [
            "63783170-b467-464d-90dd-d002ff1ecaaf"
          ],
          "value": "What types of documents can I upload?"
        },
        "content": {
          "chunk_references": [
            "0d956d57-f0fa-4631-895e-1cc94ea7cef6"
          ],
          "value": "metR supports common documentation formats such as Word and HTML. Unsupported formats cannot be processed until converted to a supported type."
        }
      },
      {
        "heading": {
          "chunk_references": [
            "e24e1218-c312-432a-aaa6-8553e1efd4a5"
          ],
          "value": "Can I edit documents after uploading?"
        },
        "content": {
          "chunk_references": [
            "223fafcc-9c52-4118-b5c0-fda12c7e68bf"
          ],
          "value": "Yes. Supported documents can be edited before or after processing, depending on the workflow. This allows teams to fix issues or refine content as needed."
        }
      },
      {
        "heading": {
          "chunk_references": [
            "ba3c7078-8234-4426-aa8d-9bfd20b70907"
          ],
          "value": "What happens if I upload an unsupported file?"
        },
        "content": {
          "chunk_references": [
            "56b90797-1f20-402d-b83b-d7503da9d0b0"
          ],
          "value": "Unsupported files cannot be processed. The system clearly indicates when a file type is not eligible for conversion."
        }
      }
    ]
  }
